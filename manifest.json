{
  "_version": "1.7.0",
  "sap.app": {
    "id": "Nathan.Shaw",
    "type": "application",
    "i18n": "i18n/i18n.properties",
    "applicationVersion": {
      "version": "1.0.0"
    },
    "title": "{{appTitle}}",
    "description": "{{appDescription}}",
    "sourceTemplate": {
      "id": "ui5template.basicSAPUI5ApplicationProject",
      "version": "1.40.12"
    },
    "dataSources": {
      "mockData": {
        "uri": "model/testData.json",
        "type": "JSON"
      },
      "S4G": {
        "uri": "/sap/opu/odata/sap/ZGW_CS_REQUEST_SRV/",
        "type": "OData",
        "settings": {
          "odataVersion": "2.0",
          "defaultCountMode": "None",
          "sizelimit": 200
        }
      }
    }
  },
  "sap.ui": {
    "technology": "UI5",
    "icons": {
      "icon": "",
      "favIcon": "",
      "phone": "",
      "phone@2": "",
      "tablet": "",
      "tablet@2": ""
    },
    "deviceTypes": {
      "desktop": true,
      "tablet": true,
      "phone": true
    },
    "supportedThemes": ["sap_hcb", "sap_belize"]
  },
  "sap.ui5": {
    "rootView": {
      "viewName": "Nathan.Shaw.view.app",
      "id": "app",
      "type": "XML"
    },
    "routing": {
      "config": {
        "routerClass": "sap.m.routing.Router",
        "viewType": "XML",
        "viewPath": "Nathan.Shaw.view",
        "controlId": "app",
        "controlAggregation": "pages",
        "transition": "flip",
        "asymc": "true",
        "bypassed": {
          "target": "notFound"
        }
      },
      "routes": [
        {
          "pattern": "",
          "name": "main",
          "target": "main"
        },
        {
          "pattern": "task/{taskId}",
          "name": "taskDetails",
          "target": "taskDetails"
        }
      ],
      "targets": {
        "main": {
          "viewName": "mainView",
          "viewId": "mainView",
          "viewLevel": 1
        },
        "taskDetails": {
          "viewName": "taskDetails",

          "viewLevel": 2
        },
        "notFound": {
          "viewId": "notFound",
          "viewName": "NotFound",
          "transition": "show"
        }
      }
    },

    "dependencies": {
      "minUI5Version": "1.30.0",
      "libs": {
        "sap.ui.core": {},
        "sap.m": {},
        "sap.ui.layout": {},
        "sap.ushell": {},
        "sap.collaboration": {},
        "sap.ui.comp": {},
        "sap.uxap": {}
      }
    },
    "contentDensities": {
      "compact": true,
      "cozy": true
    },
    "models": {
      "i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "settings": {
          "bundleName": "Nathan.Shaw.i18n.i18n"
        }
      },

      "mockDataModel": {
        "dataSource": "mockData",
        "type": "sap.ui.model.json.JSONModel"
      },
      "myService": {
        "dataSource": "S4G",
        "settings": {
          "defaultCountMode": "None",

          "useBatch": false,
          "defaultUpdateMethod": "Put"
        }
      }
    },
    "handleValidation": true,
    "resources": {
      "css": [
        {
          "uri": "css/style.css"
        }
      ],
      "js": [
        {
          "uri": "util/xlsx.js"
        }
      ]
    }
  }
}
